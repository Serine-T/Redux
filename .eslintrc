{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "semi": "error",
      "quotes": ["error", "single"],
      "indent": ["error", 2],
      "no-multiple-empty-lines": [1, {"max": 1, "maxEOF": 1}],
      "eol-last": 1,
      "newline-before-return": "error"
    },
    "reportUnusedDisableDirectives": true
}
